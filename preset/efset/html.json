{
  "__ref": "https://github.com/beautify-web/js-beautify#css--html",
  "braceStyle": "collapse",
  "indentChar": " ",
  "indentScripts": "keep",
  "indentSize": 4,
  "maxPreserveNewlines": 2,
  "preserveNewlines": true,
  "unformatted": [
    "a",
    "span",
    "img",
    "bdo",
    "em",
    "strong",
    "dfn",
    "code",
    "samp",
    "kbd",
    "var",
    "cite",
    "abbr",
    "acronym",
    "q",
    "sub",
    "sup",
    "tt",
    "i",
    "b",
    "big",
    "small",
    "u",
    "s",
    "strike",
    "font",
    "ins",
    "del",
    "pre",
    "label",
    "address",
    "dt",
    "h1",
    "h2",
    "h3",
    "h4",
    "h5",
    "h6"
  ],
  "wrapAttributes": "auto",
  "wrapAttributesIndentSize": 2,
  "wrapLineLength": 0
}